      "view/item/context": [
        {
          "command": "ardunno.selectBoard",
          "when": "view == ardunno.currentSketch && viewItem == board",
          "group": "inline"
        },
        {
          "command": "ardunno.selectPort",
          "when": "view == ardunno.currentSketch && viewItem == port",
