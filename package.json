        },
        {
          "command": "ardunno.selectProgrammer",
          "when": "view == ardunno.currentSketch && viewItem == programmer",
          "group": "inline"
        },
        {
          "command": "ardunno.selectConfigOption",
          "when": "view == ardunno.currentSketch && (viewItem == configOption || viewItem == dirtyConfigOption)",
          "group": "inline"
        },
        {
          "command": "ardunno.resetConfigOption",
          "when": "view == ardunno.currentSketch && viewItem == dirtyConfigOption",
          "group": "inline"
        },
        {
          "command": "ardunno.task.runFromTree",
          "when": "view == ardunno.currentSketch && viewItem == taskIdle",
          "group": "inline"
        },
        {
          "command": "ardunno.task.stopFromTree",
          "when": "view == ardunno.currentSketch && viewItem == taskRunning",
          "group": "inline"
        },
        {
          "command": "ardunno.tool.runFromTree",
          "when": "view == ardunno.currentSketch && viewItem == tool",
          "group": "inline"
        },
        {
          "command": "ardunno.addSketchFolderToWorkspace",
