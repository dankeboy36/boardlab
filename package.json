{
  "name": "vscode-ardunno",
  "displayName": "ardunno",
  "version": "0.0.1-beta.1",
  "description": "Unofficial Arduino extension for VS Code",
  "categories": [
    "Other"
  ],
  "bugs": {
    "url": "https://github.com/dankeboy36/vscode-ardunno/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dankeboy36/vscode-ardunno.git"
  },
  "license": "MIT",
  "author": "dankeboy36",
  "main": "./out/extension.js",
  "workspaces": [
    "packages/protocol",
    "packages/servers/portino-bridge",
    "packages/webviews/base",
    "packages/webviews/monitor-shared",
    "packages/webviews/resources",
    "packages/webviews/examples",
    "packages/webviews/monitor",
    "packages/webviews/plotter",
    "packages/webviews/platforms",
    "packages/webviews/libraries",
    "packages/webviews/profiles"
  ],
  "scripts": {
    "precompile": "npm run build --workspaces",
    "compile": "tsc -p ./",
    "postcompile": "copyfiles -u 3 \"packages/servers/portino-bridge/out/**\" out/portino-bridge",
    "download:builtins": "scripts/fetch-builtins.sh --tag 1.10.2 --target resources/arduino-examples",
    "format": "prettier --write .",
    "postinstall": "copyfiles -f ./node_modules/ardunno-cli/schemas/arduino-cli-schema.json ./schemas",
    "lint": "eslint packages --ext js,jsx,ts,tsx",
    "lint:fix": "eslint packages --ext js,jsx,ts,tsx --fix",
    "prereset": "npm run reset --workspaces",
    "reset": "rimraf schemas out node_modules .vscode-test",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js",
    "test:unit": "vitest run",
    "vscode:prepublish": "npm run compile",
    "watch": "tsc -watch -p ./"
  },
