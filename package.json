    "problemMatchers": [
      {
        "name": "ardunno",
        "fileLocation": "absolute",
        "source": "ino",
        "pattern": {
          "regexp": "^(.*?):(\\d+):(\\d*):?\\s+(?:fatal\\s+)?(note|warning|error):\\s+(.*)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "severity": 4,
          "message": 5,
          "loop": true
        }
      }
    ],
    "submenus": [
      {
        "id": "ardunno.libraries.filter.type",
        "label": "Type"
      },
      {
        "id": "ardunno.libraries.filter.topic",
        "label": "Topic"
      },
      {
        "id": "ardunno.platforms.filter.type",
        "label": "Type"
      }
    ],
    "taskDefinitions": [
      {
        "type": "ardunno",
        "required": [
          "command"
        ],
        "properties": {
          "command": {
            "type": "string",
            "description": "The Ardunno command to execute. Supported commands are 'compile', 'upload', 'burn-bootloader', 'export-binary', 'archive-sketch', 'upload-using-programmer', 'get-board-info', and 'reload-board-data'."
          },
          "sketchPath": {
            "type": "string",
            "description": "The path to the sketch to compile.",
            "default": "${command:ardunno.pickSketch}"
          },
          "fqbn": {
            "type": "string",
            "description": "Fully Qualified Board Name (FQBN) for the compilation and upload or to disambiguate if more than one platform provides the pluggable monitor for a given port protocol. For example, 'arduino:avr:uno'.",
            "default": "${command:ardunno.pickBoard}"
          },
          "port": {
            "type": "string",
            "description": "TODO! The qualified name of the port. The port protocol and the port address separated by a colon (`:`). The address can consists of the host and port separated by an additional colon. For example, 'serial:/dev/cu.usbmodem14201' or 'teensy:/usb:14300000'",
            "default": "${command:ardunno.pickPort}"
          },
          "programmer": {
            "type": "string",
            "description": "The technical ID of the programmer to use"
          }
        }
      }
    ],
    "views": {
      "ardunnoSketches": [
        {
          "id": "ardunno.currentSketch",
          "name": "Current Sketch",
          "contextualTitle": "Current Sketch",
          "icon": "$(circuit-board)"
