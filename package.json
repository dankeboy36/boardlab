    "configuration": {
      "title": "ardunno",
      "properties": {
        "ardunno.monitor.lineEnding": {
          "type": "string",
          "enum": [
            "none",
            "lf",
            "cr",
            "crlf"
          ],
          "default": "crlf",
          "markdownDescription": "Line ending appended to messages sent from the Monitor and Plotter inputs."
        },
        "ardunno.monitor.bridgePort": {
          "type": "number",
          "default": 55888,
          "markdownDescription": "Preferred TCP port for the Portino bridge server used to connect VS Code instances. If another process responds on this port with an unexpected protocol, the extension will warn and will not start its own bridge.",
          "minimum": 55000
        },
        "ardunno.monitor.bridgeMode": {
          "type": "string",
          "enum": [
            "external-process",
            "in-process"
          ],
          "default": "external-process",
          "markdownDescription": "Controls how the Portino bridge server is started. Use 'in-process' to run it inside the VS Code extension host for easier debugging. Changing this setting requires reloading the window."
        },
        "ardunno.cli.configPath": {
          "type": "string",
          "markdownDescription": "Absolute path to the Arduino CLI configuration file used by ardunno. Leave empty to auto-detect."
        },
        "ardunno.compile.verbose": {
          "type": "boolean",
          "markdownDescription": "True for verbose compile output. False by default",
          "default": false
        },
        "ardunno.compile.warnings": {
          "enum": [
            "None",
            "Default",
            "More",
            "All"
          ],
          "default": "None",
          "markdownDescription": "Tells gcc which warning level to use. It's 'None' by default"
        },
        "ardunno.upload.verbose": {
          "type": "boolean",
          "markdownDescription": "True for verbose upload output. False by default.",
          "default": false
        }
      }
    },
    "customEditors": [
      {
        "viewType": "ardunno.monitorEditor",
        "displayName": "Monitor",
        "selector": [
          {
            "scheme": "monitor",
            "filenamePattern": "**/*"
          }
        ],
        "priority": "option"
      },
      {
        "viewType": "ardunno.plotterEditor",
        "displayName": "Plotter",
        "selector": [
          {
            "scheme": "monitor",
            "filenamePattern": "**/*"
          }
        ],
        "priority": "option"
      },
      {
        "viewType": "ardunno.profilesEditor",
        "displayName": "Profiles",
        "selector": [
          {
            "scheme": "file",
            "filenamePattern": "**/sketch.yaml"
          }
        ],
        "priority": "default"
      }
    ],
    "jsonValidation": [
      {
        "fileMatch": "arduino-cli.json",
        "url": "./schemas/arduino-cli-schema.json"
      }
    ],
    "menus": {
