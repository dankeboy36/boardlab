{
  "name": "@boardlab/portino-bridge",
  "version": "0.0.0",
  "type": "commonjs",
  "exports": {
    ".": {
      "import": "./out/index.js",
      "require": "./out/index.js"
    },
    "./server": {
      "import": "./out/server.js",
      "require": "./out/server.js"
    },
    "./cliBridge": {
      "import": "./out/cliBridge.js",
      "require": "./out/cliBridge.js"
    },
    "./mockCliBridge": {
      "import": "./out/mockCliBridge.js",
      "require": "./out/mockCliBridge.js"
    },
    "./mockServer": {
      "import": "./out/mockServer.js",
      "require": "./out/mockServer.js"
    }
  },
  "main": "./out/index.js",
  "bin": {
    "portino-bridge": "./out/serviceMain.js"
  },
  "files": [
    "out"
  ],
  "scripts": {
    "prebuild": "npm --prefix ../../protocol run build",
    "build": "tsc -p ./",
    "clean": "rimraf out",
    "pretest": "npm run build",
    "test": "vitest --config ./vitest.config.js",
    "pretest:ci": "npm run build",
    "test:ci": "vitest run --config ./vitest.config.js"
  },
  "dependencies": {
    "@boardlab/protocol": "0.0.0",
    "ardunno-cli": "^1.1.1",
    "boards-list": "^2.0.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "fqbn": "^1.3.0",
    "nice-grpc": "^2.1.7",
    "p-defer": "^3.0.0",
    "vscode-ws-jsonrpc": "^3.4.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "rimraf": "^6.0.1",
    "typescript": "^5.1.x",
    "vitest": "^4.0.16"
  }
}
